*Right Sub Door Activate Script

scrp 2 2 50203 1

	lock

	doif name "open" eq 1

		frat 3

		anim [3 2 1 0]

		door grap 8758 48931 grap 8794 48931 50

		wait 7

		door grap 8758 48931 grap 8794 48931 0

		wait 7

		setv name "open" 0

	elif name "open" eq 0

*if it's flooded, close other door

		doif rtyp grap 8439 48936 eq 9

			enum 2 2 50202

				doif name "open" eq 1

					mesg writ targ 0

					targ ownr

					wait 15

				endi

			next

			gsub empty

			wait 20

			targ ownr

			anim [0 1 2 3]

			over

		else

			anim [0 1 2 3]

			over

		endi

		door grap 8758 48931 grap 8794 48931 100

		wait 7

		setv name "open" 1

	endi



	sndc "m_a4"



	subr empty

		rtyp grap 8439 48936 0

		rtyp grap 8478 48936 0

		rtyp grap 8579 48936 0

		rtyp grap 8758 48936 0

		enum 1 11 50200

			mesg writ targ 1001

		next

	retn

endm



*Left Sub Door Activate Script

scrp 2 2 50202 1

	lock

*If you open the door to the ocean, close the other door first!

	doif name "open" eq 0

*enum 2 2 50203

*next

		doif rtyp grap 8439 48936 ne 9

			enum 2 2 50203

				doif name "open" eq 1

					mesg writ targ 0

					targ ownr

					wait 15

				endi

			next



			gsub flood

			lock

			targ ownr

			wait 20

			targ ownr

		endi

		frat 3

		anim [0 1 2 3]

		over

		door grap 8425 48945 grap 8340 48945 100

		setv name "open" 1



	elif name "open" eq 1

		frat 3

		anim [3 2 1 0]

		over

		door grap 8425 48945 grap 8340 48945 0

		setv name "open" 0

	endi

	sndc "m_a4"

	unlk

	subr flood

		rtyp grap 8439 48936 9

		rtyp grap 8478 48936 9

		rtyp grap 8579 48936 9

		rtyp grap 8758 48936 9

		enum 1 11 50200

			mesg writ targ 1001

		next

	retn



endm









