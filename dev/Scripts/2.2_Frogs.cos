*VERY simple.
scrp 2 15 50217 9
	addv ov01 1

*become a frog!
	doif ov01 gt ov10
		setv va00 posl
		setv va01 post
*		subv va00 0
		subv va01 15

		setv va10 plne

		new: simp 2 15 50216 "moe_C2toDS_frogs" 105 0 va10
		mvsf va00 va01
		kill ownr
	endi

*only go in random directions if there are no walls nearby
	gsub obst

	doif va50 = 0
		doif rand 0 500 gt 250
*           Left Right
			setv ov04 rand 0 1
*           Up Down
			setv ov05 rand 0 1
		endi

		doif ov05 eq 0
			gsub up
		else
			gsub down
		endi

		doif ov04 eq 0
			gsub left
		else
			gsub right
		endi
	endi

	gsub checksurface


	subr up
*		gsub checksurface
		subv vely ov07
	retn

	subr down
		addv vely ov07
	retn

	subr left
		mira 0
		subv velx ov06
	retn

	subr right
		mira 1
		addv velx ov06
	retn

	subr checksurface
		setv va00 post
		subv va00 20
		doif rtyp grap posx va00 ne 8 and rtyp grap posx va00 ne 9
			doif vely lt 0.0
				setv vely 0
			endi
			setv ov05 1
		endi

		doif rtyp grap posx posb ne 8 and rtyp grap posx posb ne 9 and grap posx posb ne -1
			accg 1
			addv ov99 1
			doif ov99 eq 8
				kill ownr
			endi
		else
			setv ov99 0
			accg 0
		endi
	retn


*obstacle checking subroutine stolen from zander fish (with tweaks)
	subr obst

		inst

		setv va50 0

		doif obst _up_ lt 10

			setv va00 ov07
			mulv va00 0.5
			addv vely va00

			setv va50 1

		endi



		doif obst down lt 20

			setv va00 ov07
			mulv va00 0.5
			subv vely va00

			setv va50 1

		endi



		doif obst left lt 10

			setv va00 ov06
*			mulv va00 0.5
			addv velx va00

			doif velx > 0
				mira 1
			endi

			setv va50 1

		endi

		doif obst rght lt 10

			setv va00 ov06
*			mulv va00 0.5
			subv velx va00

			doif velx < 0
				mira 0
			endi

			setv va50 1

		endi

	retn


endm

