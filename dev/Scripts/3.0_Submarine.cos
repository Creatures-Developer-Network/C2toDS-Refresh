scrp 3 1 50202 1001

	lock

	doif ov00 eq 0

		gsub right_to_left

	elif ov00 eq 1

		gsub left_to_bottom

	elif ov00 eq 2

		gsub bottom_to_right

	endi

	unlk

	stop



	subr right_to_left

		part 2

		frat 2

		anim [0 1 0 1 0 1 0]

		over

		targ ownr

		gpas 4 0 0 1

		part 5

		frat 3

		anim [0 1 2 3 2 1 255]

		part 4

		frat 3

		anim [0 1 2 3 2 1 255]

