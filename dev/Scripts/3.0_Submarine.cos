		part 5

		pose 0

		part 4

		pose 0

		targ ownr

		dpas 4 0 0

		setv ov00 1

		attr 12

		etch 4 0 0

			setv vely -10

		next

	retn



	subr breathe

		epas 4 0 0

			chem 29 1

		next

		targ ownr

	retn



