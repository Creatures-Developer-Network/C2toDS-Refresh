			gsub breathe

			gsub breathe

		untl posl le 8120

		snde "sub2"

		part 5

		frat 3

		anim [0 1 2 3 2 1 255]

		setv velx 0

		setv vely -5

		loop

			wait 1

			gsub breathe

			gsub breathe

		untl posb le 48750

		setv vely -2.5

		snde "sub3"

		loop

			wait 1

			gsub breathe

			gsub breathe

		untl posb le 48693

		setv vely 0

		setv velx 5

		snde "sub2"

		part 4

		pose 0

		loop

			wait 1

			gsub breathe

			gsub breathe

		untl posr ge 9100

		setv velx 2.5

		snde "sub1"

		loop

			wait 1

			gsub breathe

			gsub breathe

		untl posr ge 9404

		setv velx 0

