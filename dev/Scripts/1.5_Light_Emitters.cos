*//Light Emitters//*

*Skyline Emitters*

	doif ov06 eq "SkyLine"

		setv ov02 1

		setv ov03 1.5000

	elif ov06 eq "Ambient"

		setv ov02 1

		setv ov03 1.0000

	elif ov06 eq "Lava"

		setv ov02 1

		setv ov03 1.0000

	elif ov06 eq "Fire"

		setv ov02 1

		setv ov03 0.4500

	elif ov06 eq "LED"

		setv ov02 1

		setv ov03 0.1500

	elif ov06 eq "Incandescent"

		setv ov02 1

		setv ov03 0.4500

	elif ov06 eq "Fluorescent"

		setv ov02 1

		setv ov03 0.6500

	endi

	emit ov02 ov03

endm

rscr


enum 1 14 50200

	kill targ

next
