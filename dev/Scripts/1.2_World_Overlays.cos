*Timer Script - Currently only used by Lights above Frog Pond (OV00 0, 1 and 2)

scrp 1 8 50200 9

	doif ov00 < 3

*randomly blink
		doif pose <> 0
			pose 0

			tick rand 3 5
		else

*0 - Left Light above Frog Pond
			doif ov00 = 0
				pose 1
*1 - Mid Light above Frog Pond
			elif ov00 = 1
				pose 2
*2 - Right Light above Frog Pond
			elif ov00 = 2
				pose 3
			endi

			setv va00 rand 2 20
			mulv va00 10
			tick va00

		endi
	endi


endm






