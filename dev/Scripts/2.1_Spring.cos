* Activate 1

scrp 2 21 50206 1

	lock

	inst

	stim writ from 97 1

	snde "boin"

	anim [0 1 2 3 4 5 6 7 0 2 3 4 6 7 0]

	etch 4 0 0

		doif mv01 eq 1

			velo -10 -90

		else

			velo rand 6 -6 rand -20 -30

		endi

	next

	targ ownr

	etch 2 0 0

*Must be Pickupable

		setv va02 attr

		andv va02 2



*And suffer physics

		setv va03 attr

		andv va03 128

*And can't float

		setv va04 attr

		andv va04 32



		doif va04 ne 32 and va02 eq 2 and va03 eq 128 and accg gt 0.0 and carr eq null and targ ne ownr

			velo rand 12 -12 rand -20 -30

		endi

	next

	unlk

endm



